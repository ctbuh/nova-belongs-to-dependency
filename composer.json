{
    "name": "ctbuh/nova-belongs-to-dependency",
    "description": "Extended Laravel Nova BelongsTo field that can depend on other fields",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ctbuh\\BelongsToDependency\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ctbuh\\BelongsToDependency\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
